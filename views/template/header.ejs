		<script src="../JS/slick.js" type="text/javascript" charset="utf-8"></script> <!-- //// -->
	   	<script type="text/javascript">
			$(function() {
			  $(window).scroll(function() {
			    if ($(this).scrollTop() > 495)  {          /* 要滑動到選單的距離 */
			       $('.menu').addClass('navFixed');   /* 幫選單加上固定效果 */
			    } else {
			      $('.menu').removeClass('navFixed'); /* 移除選單固定效果 */
			    }
			  });
			});
		</script>
		<style>
			.navFixed {
			  z-index: 10;          
			  position: fixed;
			  top: 0;
			  left: 0px;
			  margin-top: 0;
			  opacity: 0.70;
			  transition: opacity .5s ease-out;
			  background-color: #fff

			}
		</style> 
		<section class="regular slider" id="sli">
			<script>
				document.getElementById("sli").innerHTML=null;
				for(let i=0;i<10;i++){
					document.getElementById("sli").innerHTML+='<div id="'+i+'" class="status">id='+i+'<br/>休息中</div>';
				}
			</script>
	  	</section>
		<div>  
		  	<script>
			    $(document).ready(function() {
			      $(".regular").slick({
			        dots: true,
			        infinite: true,
			        slidesToShow: 3,
			        slidesToScroll: 1
			      });
			    });
			</script>
